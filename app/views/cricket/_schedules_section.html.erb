<section id="schedules">
	<div class="container">
		<div class="row text-primary col-sm-10 col-sm-offset-1">
			<h3>Scedules</h3>
		</div>
		<div class="row col-sm-10 col-sm-offset-1">

			<div class="cl-scdl-box">
				<% @matchCalendar['data']['data'].each do |type| %>

				<div class="cl-scdl-lst">
					<div class="cl-scdl-date">
						<b class="cl-scdl-hdln"><b class="cl-scdl-hdln-ancr"
						title="<%= type['date'].html_safe %>"><%= type['date'].html_safe %></b></b>
					</div>
					<div class="cl-scdl-cntr">
						<div class="cl-scdl-intr">
							<%= type['name'].html_safe %>	
						</div>
					</div>					
				</div>
				<% end %>
			</div>

		</div>
	</div>
</section>

